{
  "manifest_version": 3,
  "name": "Instagram Leads",
  "description": "Base Level Extension",
  "version": "1.0",
  "action": {
    "default_icon": "ig-icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*.instagram.com/*",
        "https://*.instagram.com/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ]
}
